<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="33dp"
    tools:context="com.example.seunghyeonpark.dailycondition.Fragment.SettingFragment"
    android:orientation="horizontal">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:background="@drawable/setting"

        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <Button
            android:background="#00ff0000"
            android:id="@+id/mail"

            android:layout_height="120dp"
            android:layout_width="100dp"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/myInfo"
            android:layout_marginBottom="36dp"
            android:layout_marginStart="10dp"
            android:action="android.intent.action.SENDTO"
            android:data="mailto:dgupsh@gmail.com"/>


        <Button
            android:layout_width="100dp"
            android:layout_height="119dp"
            android:id="@+id/alarm"
            android:background="#00ff0000"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/version"
            android:layout_marginBottom="171dp" />

        <Button
            android:layout_width="100dp"
            android:layout_height="115dp"
            android:id="@+id/version"
            android:background="#00ff0000"
            android:layout_marginStart="41dp"
            android:layout_marginTop="24dp"
            android:layout_below="@+id/myInfo"
            android:layout_toEndOf="@+id/myInfo" />

        <Button
            android:layout_width="120dp"
            android:id="@+id/myInfo"
            android:layout_height="110dp"
            android:background="#00ff0000"
            android:layout_marginStart="53dp"
            android:layout_alignBottom="@+id/alarm"
            android:layout_alignParentStart="true" />
    </RelativeLayout>


</RelativeLayout>
